{
  "name": "electron-vite-vue3",
  "private": true,
  "version": "1.0.0",
  "description": "electron vite vue3 template",
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "vue",
    "vue devtools",
    "electron-vue",
    "electron-vite"
  ],
  "main": "electron/index.js",
