    "electron-log": "^5.1.5",
    "element-plus": "^2.7.5",
    "iconv-lite": "^0.6.3",
    "resolve": "^1.22.8",
    "vue": "^3.2.25",
    "win-version-info": "^6.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^21.4.4",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.1.2",
    "electron-reloader": "^1.2.3",
    "sass": "^1.49.9",
